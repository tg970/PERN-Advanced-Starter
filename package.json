{
  "name": "pern-advanced-starter",
  "version": "1.0.0",
  "description": "Advanced PERN stack starter app",
  "engines": {
    "npm": "6.1.0"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "npm run build && git add . && git commit -m \"deployment build\" && git push origin master && git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tg970/PERN-Advanced-Starter.git"
  },
  "author": "Tyler Goodman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tg970/PERN-Advanced-Starter/issues"
  },
  "homepage": "https://github.com/tg970/PERN-Advanced-Starter#readme",
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "pg-monitor": "^1.0.0",
    "pg-promise": "^8.4.5",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.3.1",
    "react-nouislider": "^2.0.1",
    "react-popper": "0.10.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.1",
    "react-swipeable-views": "^0.12.15",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
