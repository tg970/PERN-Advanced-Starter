    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
